form(ng-submit="submit()", ng-controller="PartController", ng-init="init()")
  div.row
    div.col-md-10
      h2 New Part
  div.row
    p
      | Description 
      input(type="text",ng-model="part.description",required)
  div.row
    p
      | Category
      input(type="text",ng-model="part.category",required)
  input(type="submit", id="submit", value="Save")
